      "url": "/health",
      "icons": [
        {
          "src": "shortcut-health.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "AI Health Coach",
      "short_name": "AI Coach",
      "description": "Get personalized health tips from your AI coach",
      "url": "/ai",
      "icons": [
        {
          "src": "shortcut-ai.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Medication Tracker",
      "short_name": "Medications",
